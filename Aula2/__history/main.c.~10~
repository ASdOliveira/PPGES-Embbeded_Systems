#include <main.h>
#include <keyboard.h>

uint8_t lerTec = 0, lerTecAtual = 0, delayCount = 0;
uint8_t port = 0x01;
void main()
{
   key_init();
   while(TRUE)
   {
    lerTec = key_read();
    if(lerTec != 0xFF)
    {
      lerTecAtual=lerTec;
    }
     
     rotate_right(&port,1);
     delay_ms(lerTecAtual);
   }

}


